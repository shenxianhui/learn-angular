{
  "name": "pro-custom",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "copy": "copyfiles -u 3 dist/%npm_config_name%/bundles/* src/assets/%npm_config_name%",
    "watch": "onchange \"dist/%npm_config_name%/bundles/*.js\" -- npm run copy --name=%npm_config_name%",
    "copy-linux": "copyfiles -u 3 dist/\"${npm_config_name}\"/bundles/* src/assets/\"${npm_config_name}\"",
    "watch-linux": "onchange \"dist/\"${npm_config_name}\"/bundles/*.js\" -- npm run copy --name=\"${npm_config_name}\""
  },
  "private": true,
