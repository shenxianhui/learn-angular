.map {
  width: 100%;
  height: 100%;
}

.container {
  display: flex;
  font-size: 14px;
  height: 100%;
  width: 100%;
}
.sidebar {
  height: 100%;
  position: relative;
  flex-basis: 15%;
  padding: 20px 0 0 20px;
}
.main-field {
  height: 100%; 
  flex-basis: 85%;
}
.main-content {
  padding: 20px;
  max-width: 99%;
  left: 16%;
  max-height:14%;
}
.row {
  display: flex;
  margin-top: 20px;
  flex-wrap: wrap;
}

.attack-des {
  position: relative;
  color: #ff435b;
  height: 30px;
  width: 150px;
}
.attack-des-icon {
  width: 20px;
  height: 20px;
  position: absolute;
  left: 0;
  top: 5px;
}
.attack-des-bg {
  position: absolute;
  left: 0;
  top: 0;
  width: 100%;
  height: 100%;
}
.attack-des-text {
  position: absolute;
  left: 25px;
  top: 0;
  width: 100%;
  height: 100%;
  line-height: 30px;
}

.left-des {
  position: relative;
  display: flex;
  color: #ffffff;
  height: 94px;
  flex-basis: 30%;
  box-sizing: border-box;
  padding-top: 20px;
}
.left-des-img {
  width: 32px;
  height: 49px;
  display: block;
  position: relative;
  right: 10px;
  bottom: 5px;
}
.left-des-title {
  font-size: 14px;
  line-height: 22px;
  margin-bottom: 9px;
  opacity: 0.7;
}
.left-des-detail {
  width: 80px;
  line-height: 22px;

}
.bottom-bg {
  left: 0;
  top: 0;
  width: 100%;
  height: 100%;
}
.block {
  width: 200px;
  height: 40px;
  margin: 0 10px;
  position: relative;
}
.block {
  bottom: 0;
}
.absolute {
  position: absolute;

}
.bottom-btn-text {
  color: #ff435b;
  line-height: 40px;
  margin-left: 20px;
}

.bottom-btn-check {
  color: #ff435b;
  line-height: 40px;
  position: absolute;
  right: 26px;
  top: 0;
  cursor:pointer;
}

.bottom-btn {
  position: absolute;
  top: 50%;
  transform: translateY(-50%);
  width: 100%;
  height: 40px;
  color: #ff435b;
}

.high-row {
  display: flex;
  color: #ffffff;
  width: 500px;
  align-items: center;
}
.high-row-icon {
  width: 20px;
  height: 20px;
}
.high-row-num {
  width: 30px;
  text-align: center;
  font-size: 14px;
}
.swiper-line {
  display: flex;
}
.swiper-line-bg {
  margin-top: 10px;
  width: 25px;
  height: 3px;
  background-color: #6a737c;
  border-radius: 10px;
  margin-left: 5px;
}
.curswiper {
  background-color: #fff;

}
.map-content {
  position: relative;
  height: 56%;
}
.map-container {
  width: 100%;
  height:100%; 
  position: relative;
}
.bottombtntextchoose {
  color: #ffffff;
}
.bottombtncheckchoose {
  color: #3786AC;
}
.left-bottom-icon {
  width: 16px;
  height: 16px;
  left: -8px;
  top: 12px;
}