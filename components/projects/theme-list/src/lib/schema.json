  "config": {
    "list": [
      {
        "attribute": "intervalColor",
        "declare": "隔列字体颜色",
        "default": "#52D2FF",
        "desc": "不传或者为空时，启用默认值"
      },
      {
        "attribute": "hoverBgColor",
        "declare": "每一行选中背景色",
        "default": "rgba(23, 46, 75, 1)",
        "desc": "不传或者为空时，启用默认值"
      }
