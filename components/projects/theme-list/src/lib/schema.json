      },
      {
        "attribute": "td",
        "declare": "表格内容，对象数组包含row和url两个属性",
        "type": "Array",
        "desc": "必传。只做展示，不做校验"
      },
      {
        "attribute": "row",
        "declare": "一行数据，字符串数组数量要与th数组数量一致",
        "type": "Array",
        "desc": "必传。只做展示，不做校验"
      },
      {
        "attribute": "url",
        "declare": "点击一行数据后以新窗口打开该url",
        "type": "String",
        "desc": "非必传，默认点击不跳转"
      }
    ],
